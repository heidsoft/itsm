    this.preferences.tableSettings[tableId] = settings;
    /* istanbul ignore next */
    cov_58a1oofdf().s[20]++;
    this.savePreferences();
  }
  subscribe(listener) {
    /* istanbul ignore next */
    cov_58a1oofdf().f[7]++;
    cov_58a1oofdf().s[21]++;
    this.listeners.push(listener);
    /* istanbul ignore next */
    cov_58a1oofdf().s[22]++;
    return () => {
      /* istanbul ignore next */
      cov_58a1oofdf().f[8]++;
      cov_58a1oofdf().s[23]++;
      this.listeners = this.listeners.filter(l => {
        /* istanbul ignore next */
        cov_58a1oofdf().f[9]++;
        cov_58a1oofdf().s[24]++;
        return l !== listener;
      });
    };
  }
  notifyListeners() {
    /* istanbul ignore next */
    cov_58a1oofdf().f[10]++;
    cov_58a1oofdf().s[25]++;
    this.listeners.forEach(listener => {
      /* istanbul ignore next */
      cov_58a1oofdf().f[11]++;
      cov_58a1oofdf().s[26]++;
      return listener(this.preferences);
    });
  }
  reset() {
    /* istanbul ignore next */
    cov_58a1oofdf().f[12]++;
    cov_58a1oofdf().s[27]++;
    this.preferences = {
      ...defaultPreferences
    };
    /* istanbul ignore next */
    cov_58a1oofdf().s[28]++;
    this.savePreferences();
  }
}
