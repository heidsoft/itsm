  // 检查是否为受保护的路由
  const isProtectedRoute =
  /* istanbul ignore next */
  (cov_b2s6zod2w().s[15]++, protectedRoutes.some(route => {
    /* istanbul ignore next */
    cov_b2s6zod2w().f[5]++;
    cov_b2s6zod2w().s[16]++;
    return pathname.startsWith(route);
  }));
  // 检查是否为公开路由
  const isPublicRoute =
  /* istanbul ignore next */
  (cov_b2s6zod2w().s[17]++, publicRoutes.some(route => {
    /* istanbul ignore next */
    cov_b2s6zod2w().f[6]++;
    cov_b2s6zod2w().s[18]++;
    return pathname.startsWith(route);
  }));
  // 如果是受保护的路由但没有token，重定向到登录页
  /* istanbul ignore next */
  cov_b2s6zod2w().s[19]++;
  if (
  /* istanbul ignore next */
  (cov_b2s6zod2w().b[2][0]++, isProtectedRoute) &&
  /* istanbul ignore next */
  (cov_b2s6zod2w().b[2][1]++, !token)) {
    /* istanbul ignore next */
    cov_b2s6zod2w().b[1][0]++;
    const loginUrl =
    /* istanbul ignore next */
    (cov_b2s6zod2w().s[20]++, new URL('/login', request.url));
    /* istanbul ignore next */
    cov_b2s6zod2w().s[21]++;
    loginUrl.searchParams.set('redirect', pathname);
    /* istanbul ignore next */
    cov_b2s6zod2w().s[22]++;
    return _server.NextResponse.redirect(loginUrl);
  } else
  /* istanbul ignore next */
  {
    cov_b2s6zod2w().b[1][1]++;
  }
  // 如果已登录用户访问公开路由，重定向到仪表盘
  cov_b2s6zod2w().s[23]++;
  if (
  /* istanbul ignore next */
  (cov_b2s6zod2w().b[4][0]++, isPublicRoute) &&
  /* istanbul ignore next */
  (cov_b2s6zod2w().b[4][1]++, token)) {
    /* istanbul ignore next */
    cov_b2s6zod2w().b[3][0]++;
    cov_b2s6zod2w().s[24]++;
    return _server.NextResponse.redirect(new URL('/dashboard', request.url));
  } else
  /* istanbul ignore next */
  {
    cov_b2s6zod2w().b[3][1]++;
  }
  // 根路径重定向
  cov_b2s6zod2w().s[25]++;
  if (pathname === '/') {
    /* istanbul ignore next */
    cov_b2s6zod2w().b[5][0]++;
    cov_b2s6zod2w().s[26]++;
    if (token) {
      /* istanbul ignore next */
      cov_b2s6zod2w().b[6][0]++;
      cov_b2s6zod2w().s[27]++;
      return _server.NextResponse.redirect(new URL('/dashboard', request.url));
    } else {
      /* istanbul ignore next */
      cov_b2s6zod2w().b[6][1]++;
      cov_b2s6zod2w().s[28]++;
      return _server.NextResponse.redirect(new URL('/login', request.url));
    }
  } else
  /* istanbul ignore next */
  {
    cov_b2s6zod2w().b[5][1]++;
  }
  cov_b2s6zod2w().s[29]++;
  return _server.NextResponse.next();
}
const config =
/* istanbul ignore next */
(cov_b2s6zod2w().s[30]++, {
  matcher: [
  /*
  * 匹配所有路径除了:
  * - api (API routes)
  * - _next/static (static files)
  * - _next/image (image optimization files)
  * - favicon.ico (favicon file)
  * - public folder
  */
  '/((?!api|_next/static|_next/image|favicon.ico|public).*)']
});
