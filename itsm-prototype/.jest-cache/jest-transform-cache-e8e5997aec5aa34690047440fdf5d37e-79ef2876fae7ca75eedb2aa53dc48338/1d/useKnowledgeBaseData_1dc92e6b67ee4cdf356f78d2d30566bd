    /* istanbul ignore next */
    (cov_2iet9f82oa().b[1][1]++, article.summary.toLowerCase().includes(searchText.toLowerCase()))) :
    /* istanbul ignore next */
    (cov_2iet9f82oa().b[0][1]++, true));
    const matchesCategory =
    /* istanbul ignore next */
    (cov_2iet9f82oa().s[26]++, categoryFilter ?
    /* istanbul ignore next */
    (cov_2iet9f82oa().b[2][0]++, article.category === categoryFilter) :
    /* istanbul ignore next */
    (cov_2iet9f82oa().b[2][1]++, true));
    /* istanbul ignore next */
    cov_2iet9f82oa().s[27]++;
    return /* istanbul ignore next */(cov_2iet9f82oa().b[3][0]++, matchesSearch) &&
    /* istanbul ignore next */
    (cov_2iet9f82oa().b[3][1]++, matchesCategory);
  }));
  /* istanbul ignore next */
  cov_2iet9f82oa().s[28]++;
  return {
    articles: filteredArticles,
    loading,
    stats,
    setSearchText,
    setCategoryFilter,
    loadArticles
  };
};
