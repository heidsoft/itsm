          className: "space-y-3",
          children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_antd.Button, {
            type: "primary",
            size: "large",
            icon: /*#__PURE__*/(0, _jsxruntime.jsx)(_lucidereact.RefreshCw, {}),
            onClick: handleRetry,
            className: "w-full",
            children: "重试"
          }), /*#__PURE__*/(0, _jsxruntime.jsx)(_antd.Button, {
            size: "large",
            icon: /*#__PURE__*/(0, _jsxruntime.jsx)(_lucidereact.ArrowRight, {}),
            onClick: () => {
              /* istanbul ignore next */
              cov_2jtax89gic().f[12]++;
              cov_2jtax89gic().s[51]++;
              return handleManualRedirect('/login');
            },
            className: "w-full",
            children: "前往登录页面"
          })]
        })]
      })
    });
  } else
  /* istanbul ignore next */
  {
    cov_2jtax89gic().b[4][1]++;
  }
  // 渲染认证状态
  cov_2jtax89gic().s[52]++;
  if (authStatus === 'authenticated') {
    /* istanbul ignore next */
    cov_2jtax89gic().b[5][0]++;
    cov_2jtax89gic().s[53]++;
    return /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
      className: "min-h-screen bg-gradient-to-br from-green-50 via-white to-emerald-50 flex items-center justify-center p-6",
      children: /*#__PURE__*/(0, _jsxruntime.jsxs)(_antd.Card, {
        className: "w-full max-w-md text-center shadow-2xl border-0",
        style: {
          borderRadius: '20px'
        },
        children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
          className: "mb-6",
          children: [/*#__PURE__*/(0, _jsxruntime.jsx)("div", {
            className: "w-20 h-20 mx-auto mb-4 bg-gradient-to-br from-green-500 to-emerald-600 rounded-2xl flex items-center justify-center shadow-lg",
            children: /*#__PURE__*/(0, _jsxruntime.jsx)(_lucidereact.CheckCircle, {
              className: "w-10 h-10 text-white"
            })
          }), /*#__PURE__*/(0, _jsxruntime.jsx)(Title, {
            level: 2,
            className: "mb-2 text-green-600",
            children: "认证成功"
          }), /*#__PURE__*/(0, _jsxruntime.jsx)(Text, {
            className: "text-gray-600",
            children: "正在跳转到仪表盘..."
          })]
        }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
          className: "mb-6",
          children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_antd.Spin, {
            size: "large"
          }), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
            className: "mt-4",
            children: /*#__PURE__*/(0, _jsxruntime.jsx)(Text, {
              className: "text-gray-500",
              children: "准备中..."
            })
          })]
        }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
          className: "space-y-3",
          children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
            className: "flex items-center justify-center space-x-2 text-sm text-green-500",
            children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_lucidereact.CheckCircle, {
              className: "w-4 h-4"
            }), /*#__PURE__*/(0, _jsxruntime.jsx)("span", {
              children: "用户身份已验证"
            })]
          }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
            className: "flex items-center justify-center space-x-2 text-sm text-green-500",
            children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_lucidereact.CheckCircle, {
              className: "w-4 h-4"
            }), /*#__PURE__*/(0, _jsxruntime.jsx)("span", {
              children: "权限检查通过"
            })]
          }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
            className: "flex items-center justify-center space-x-2 text-sm text-blue-500",
            children: [/*#__PURE__*/(0, _jsxruntime.jsx)("div", {
              className: "w-2 h-2 bg-blue-400 rounded-full animate-pulse"
            }), /*#__PURE__*/(0, _jsxruntime.jsx)("span", {
              children: "即将跳转"
            })]
          })]
        })]
      })
    });
  } else
  /* istanbul ignore next */
  {
    cov_2jtax89gic().b[5][1]++;
  }
  // 渲染未认证状态
  cov_2jtax89gic().s[54]++;
  return /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
    className: "min-h-screen bg-gradient-to-br from-orange-50 via-white to-amber-50 flex items-center justify-center p-6",
    children: /*#__PURE__*/(0, _jsxruntime.jsxs)(_antd.Card, {
      className: "w-full max-w-md text-center shadow-2xl border-0",
      style: {
        borderRadius: '20px'
      },
      children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
        className: "mb-6",
        children: [/*#__PURE__*/(0, _jsxruntime.jsx)("div", {
          className: "w-20 h-20 mx-auto mb-4 bg-gradient-to-br from-orange-500 to-amber-600 rounded-2xl flex items-center justify-center shadow-lg",
          children: /*#__PURE__*/(0, _jsxruntime.jsx)(_lucidereact.Shield, {
            className: "w-10 h-10 text-white"
          })
        }), /*#__PURE__*/(0, _jsxruntime.jsx)(Title, {
          level: 2,
          className: "mb-2 text-orange-600",
          children: "需要登录"
        }), /*#__PURE__*/(0, _jsxruntime.jsx)(Text, {
          className: "text-gray-600",
          children: "请先登录以访问系统"
        })]
      }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
        className: "mb-6",
        children: [/*#__PURE__*/(0, _jsxruntime.jsx)("div", {
          className: "w-16 h-16 mx-auto mb-4",
          children: /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
            className: "w-full h-full border-4 border-orange-200 border-t-orange-500 rounded-full animate-spin"
          })
        }), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
          className: "mt-4",
          children: /*#__PURE__*/(0, _jsxruntime.jsx)(Text, {
            className: "text-gray-500",
            children: "即将跳转到登录页面..."
          })
        })]
      }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
        className: "space-y-3",
        children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
          className: "flex items-center justify-center space-x-2 text-sm text-orange-500",
          children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_lucidereact.AlertCircle, {
