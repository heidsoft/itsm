            cov_2jtax89gic().s[24]++;
            router.push('/dashboard');
          }, 1000);
        } else {
          /* istanbul ignore next */
          cov_2jtax89gic().b[0][1]++;
          cov_2jtax89gic().s[25]++;
          setAuthStatus('unauthenticated');
          // 延迟重定向，让用户看到状态
          /* istanbul ignore next */
          cov_2jtax89gic().s[26]++;
          setTimeout(() => {
            /* istanbul ignore next */
            cov_2jtax89gic().f[6]++;
            cov_2jtax89gic().s[27]++;
            router.push('/login');
          }, 1000);
        }
      } catch (err) {
        /* istanbul ignore next */
        cov_2jtax89gic().s[28]++;
        setError(err instanceof Error ?
        /* istanbul ignore next */
        (cov_2jtax89gic().b[1][0]++, err.message) :
        /* istanbul ignore next */
        (cov_2jtax89gic().b[1][1]++, '认证检查失败'));
        /* istanbul ignore next */
        cov_2jtax89gic().s[29]++;
        setAuthStatus('unauthenticated');
      } finally {
        /* istanbul ignore next */
        cov_2jtax89gic().s[30]++;
        setLoading(false);
      }
    };
    /* istanbul ignore next */
    cov_2jtax89gic().s[31]++;
    checkAuthAndRedirect();
  }, [router]);
  /* istanbul ignore next */
  cov_2jtax89gic().s[32]++;
  const handleRetry = () => {
    /* istanbul ignore next */
    cov_2jtax89gic().f[7]++;
    cov_2jtax89gic().s[33]++;
    setLoading(true);
    /* istanbul ignore next */
    cov_2jtax89gic().s[34]++;
    setError(null);
    /* istanbul ignore next */
    cov_2jtax89gic().s[35]++;
    setAuthStatus('checking');
    // 重新检查认证状态
    /* istanbul ignore next */
    cov_2jtax89gic().s[36]++;
    setTimeout(() => {
      /* istanbul ignore next */
      cov_2jtax89gic().f[8]++;
      cov_2jtax89gic().s[37]++;
      if (_authservice.AuthService.isAuthenticated()) {
        /* istanbul ignore next */
        cov_2jtax89gic().b[2][0]++;
        cov_2jtax89gic().s[38]++;
        setAuthStatus('authenticated');
        /* istanbul ignore next */
        cov_2jtax89gic().s[39]++;
        setTimeout(() => {
          /* istanbul ignore next */
          cov_2jtax89gic().f[9]++;
          cov_2jtax89gic().s[40]++;
          return router.push('/dashboard');
        }, 1000);
      } else {
        /* istanbul ignore next */
        cov_2jtax89gic().b[2][1]++;
        cov_2jtax89gic().s[41]++;
        setAuthStatus('unauthenticated');
        /* istanbul ignore next */
        cov_2jtax89gic().s[42]++;
        setTimeout(() => {
          /* istanbul ignore next */
          cov_2jtax89gic().f[10]++;
          cov_2jtax89gic().s[43]++;
          return router.push('/login');
        }, 1000);
      }
      /* istanbul ignore next */
      cov_2jtax89gic().s[44]++;
      setLoading(false);
    }, 800);
  };
  /* istanbul ignore next */
  cov_2jtax89gic().s[45]++;
  const handleManualRedirect = path => {
    /* istanbul ignore next */
    cov_2jtax89gic().f[11]++;
    cov_2jtax89gic().s[46]++;
    router.push(path);
  };
  // 渲染加载状态
  /* istanbul ignore next */
  cov_2jtax89gic().s[47]++;
  if (loading) {
    /* istanbul ignore next */
    cov_2jtax89gic().b[3][0]++;
    cov_2jtax89gic().s[48]++;
    return /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
      className: "min-h-screen bg-gradient-to-br from-blue-50 via-white to-indigo-50 flex items-center justify-center p-6",
      children: /*#__PURE__*/(0, _jsxruntime.jsxs)(_antd.Card, {
        className: "w-full max-w-md text-center shadow-2xl border-0",
        style: {
          borderRadius: '20px'
        },
        children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
          className: "mb-6",
          children: [/*#__PURE__*/(0, _jsxruntime.jsx)("div", {
            className: "w-20 h-20 mx-auto mb-4 bg-gradient-to-br from-blue-500 to-indigo-600 rounded-2xl flex items-center justify-center shadow-lg",
            children: /*#__PURE__*/(0, _jsxruntime.jsx)(_lucidereact.Shield, {
              className: "w-10 h-10 text-white"
            })
          }), /*#__PURE__*/(0, _jsxruntime.jsx)(Title, {
            level: 2,
