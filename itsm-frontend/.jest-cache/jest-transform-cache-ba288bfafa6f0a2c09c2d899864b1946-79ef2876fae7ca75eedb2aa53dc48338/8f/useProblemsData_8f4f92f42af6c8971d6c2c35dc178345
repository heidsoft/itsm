      setPagination(prev => {
        /* istanbul ignore next */
        cov_1py3g65ub5().f[4]++;
        cov_1py3g65ub5().s[27]++;
        return {
          ...prev,
          total: response.total
        };
      });
    } catch {
      /* istanbul ignore next */
      cov_1py3g65ub5().s[28]++;
      _antd.message.error('获取问题列表失败');
    } finally {
      /* istanbul ignore next */
      cov_1py3g65ub5().s[29]++;
      setLoading(false);
    }
  };
  /* istanbul ignore next */
  cov_1py3g65ub5().s[30]++;
  const fetchStats = async () => {
    /* istanbul ignore next */
    cov_1py3g65ub5().f[5]++;
    cov_1py3g65ub5().s[31]++;
    try {
      /* istanbul ignore next */
      cov_1py3g65ub5().s[32]++;
      // TODO: 实现统计数据获取
      console.log('获取统计数据');
    } catch {
      /* istanbul ignore next */
      cov_1py3g65ub5().s[33]++;
      _antd.message.error('获取统计数据失败');
    }
  };
  /* istanbul ignore next */
  cov_1py3g65ub5().s[34]++;
  const handleTableChange = (page, pageSize) => {
    /* istanbul ignore next */
    cov_1py3g65ub5().f[6]++;
    cov_1py3g65ub5().s[35]++;
    setPagination(prev => {
      /* istanbul ignore next */
      cov_1py3g65ub5().f[7]++;
      cov_1py3g65ub5().s[36]++;
      return {
        ...prev,
        current: page,
        pageSize:
        /* istanbul ignore next */
        (cov_1py3g65ub5().b[2][0]++, pageSize) ||
        /* istanbul ignore next */
        (cov_1py3g65ub5().b[2][1]++, prev.pageSize)
      };
    });
  };
  /* istanbul ignore next */
  cov_1py3g65ub5().s[37]++;
  return {
    problems,
    loading,
    filter,
    setFilter,
    searchText,
    setSearchText,
    pagination,
    handleTableChange,
    stats,
    fetchProblems
  };
};
