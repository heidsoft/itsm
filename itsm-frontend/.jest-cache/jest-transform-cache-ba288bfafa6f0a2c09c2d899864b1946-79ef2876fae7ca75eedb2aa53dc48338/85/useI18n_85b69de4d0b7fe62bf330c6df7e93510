  cov_2ckfji5xv7().f[1]++;
  const [language, setLanguage] =
  /* istanbul ignore next */
  (cov_2ckfji5xv7().s[6]++, (0, _react.useState)('zh-CN'));
  // 初始化语言设置
  /* istanbul ignore next */
  cov_2ckfji5xv7().s[7]++;
  (0, _react.useEffect)(() => {
    /* istanbul ignore next */
    cov_2ckfji5xv7().f[2]++;
    const prefs =
    /* istanbul ignore next */
    (cov_2ckfji5xv7().s[8]++, _userpreferences.userPreferences.get());
    const currentLang =
    /* istanbul ignore next */
    (cov_2ckfji5xv7().s[9]++, prefs.language === 'en-US' ?
    /* istanbul ignore next */
    (cov_2ckfji5xv7().b[0][0]++, 'en-US') :
    /* istanbul ignore next */
    (cov_2ckfji5xv7().b[0][1]++, 'zh-CN'));
    /* istanbul ignore next */
    cov_2ckfji5xv7().s[10]++;
    setLanguage(currentLang);
    // 订阅语言变化
    const unsubscribe =
    /* istanbul ignore next */
    (cov_2ckfji5xv7().s[11]++, _userpreferences.userPreferences.subscribe(prefs => {
      /* istanbul ignore next */
      cov_2ckfji5xv7().f[3]++;
      const newLang =
      /* istanbul ignore next */
      (cov_2ckfji5xv7().s[12]++, prefs.language === 'en-US' ?
      /* istanbul ignore next */
      (cov_2ckfji5xv7().b[1][0]++, 'en-US') :
      /* istanbul ignore next */
      (cov_2ckfji5xv7().b[1][1]++, 'zh-CN'));
      /* istanbul ignore next */
      cov_2ckfji5xv7().s[13]++;
      setLanguage(newLang);
    }));
    /* istanbul ignore next */
    cov_2ckfji5xv7().s[14]++;
    return unsubscribe;
  }, []);
  /**
  * 获取翻译文本
  * @param key 翻译键，支持嵌套路径，如 'dashboard.title'
  * @param params 可选的参数对象，用于替换翻译文本中的占位符，如 {name: 'John'} 会替换 {name}
  */
  const t =
  /* istanbul ignore next */
  (cov_2ckfji5xv7().s[15]++, (0, _react.useCallback)((key, params) => {
    /* istanbul ignore next */
    cov_2ckfji5xv7().f[4]++;
    const keys =
    /* istanbul ignore next */
    (cov_2ckfji5xv7().s[16]++, key.split('.'));
    let value =
    /* istanbul ignore next */
    (cov_2ckfji5xv7().s[17]++, _translations.translations[language]);
    /* istanbul ignore next */
    cov_2ckfji5xv7().s[18]++;
    for (const k of keys) {
      /* istanbul ignore next */
      cov_2ckfji5xv7().s[19]++;
      if (
      /* istanbul ignore next */
      (cov_2ckfji5xv7().b[3][0]++, value) &&
      /* istanbul ignore next */
      (cov_2ckfji5xv7().b[3][1]++, typeof value === 'object') &&
      /* istanbul ignore next */
      (cov_2ckfji5xv7().b[3][2]++, k in value)) {
        /* istanbul ignore next */
        cov_2ckfji5xv7().b[2][0]++;
        cov_2ckfji5xv7().s[20]++;
        value = value[k];
      } else {
        /* istanbul ignore next */
        cov_2ckfji5xv7().b[2][1]++;
        cov_2ckfji5xv7().s[21]++;
        // 如果找不到翻译，尝试使用中文作为后备
        value = _translations.translations['zh-CN'];
        /* istanbul ignore next */
        cov_2ckfji5xv7().s[22]++;
        for (const fallbackKey of keys) {
          /* istanbul ignore next */
          cov_2ckfji5xv7().s[23]++;
          if (
          /* istanbul ignore next */
          (cov_2ckfji5xv7().b[5][0]++, value) &&
          /* istanbul ignore next */
          (cov_2ckfji5xv7().b[5][1]++, typeof value === 'object') &&
          /* istanbul ignore next */
          (cov_2ckfji5xv7().b[5][2]++, fallbackKey in value)) {
            /* istanbul ignore next */
            cov_2ckfji5xv7().b[4][0]++;
            cov_2ckfji5xv7().s[24]++;
            value = value[fallbackKey];
          } else {
            /* istanbul ignore next */
            cov_2ckfji5xv7().b[4][1]++;
            cov_2ckfji5xv7().s[25]++;
            return key; // 如果还是找不到，返回原始键
          }
        }
        /* istanbul ignore next */
        cov_2ckfji5xv7().s[26]++;
        break;
      }
    }
    let result =
    /* istanbul ignore next */
    (cov_2ckfji5xv7().s[27]++, typeof value === 'string' ?
    /* istanbul ignore next */
    (cov_2ckfji5xv7().b[6][0]++, value) :
    /* istanbul ignore next */
    (cov_2ckfji5xv7().b[6][1]++, key));
    // 替换参数占位符
    /* istanbul ignore next */
    cov_2ckfji5xv7().s[28]++;
    if (params) {
      /* istanbul ignore next */
      cov_2ckfji5xv7().b[7][0]++;
      cov_2ckfji5xv7().s[29]++;
      Object.keys(params).forEach(paramKey => {
        /* istanbul ignore next */
        cov_2ckfji5xv7().f[5]++;
        const regex =
        /* istanbul ignore next */
        (cov_2ckfji5xv7().s[30]++, new RegExp(`\\{${paramKey}\\}`, 'g'));
        /* istanbul ignore next */
        cov_2ckfji5xv7().s[31]++;
        result = result.replace(regex, String(params[paramKey]));
      });
    } else
    /* istanbul ignore next */
    {
      cov_2ckfji5xv7().b[7][1]++;
    }
    cov_2ckfji5xv7().s[32]++;
    return result;
  }, [language]));
  /**
  * 切换语言
  */
  const changeLanguage =
  /* istanbul ignore next */
  (cov_2ckfji5xv7().s[33]++, (0, _react.useCallback)(lang => {
    /* istanbul ignore next */
    cov_2ckfji5xv7().f[6]++;
    cov_2ckfji5xv7().s[34]++;
    _userpreferences.userPreferences.update({
      language: lang
    });
  }, []));
  /* istanbul ignore next */
  cov_2ckfji5xv7().s[35]++;
  return {
    t,
    language,
    changeLanguage
  };
}
